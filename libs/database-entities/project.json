{
  "name": "database-entities",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/database-entities/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-migration-datasource": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/database-entities-datasource",
        "main": "libs/database-entities/src/lib/datasource.ts",
        "tsConfig": "libs/database-entities/tsconfig.lib.json",
        "webpackConfig": "libs/database-entities/webpack.config.migrations.js"
      }
    },
    "migration-generate": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "cwd": ".",
        "commands": [
          "typeorm -d dist/libs/database-entities-datasource/main.js migration:generate"
        ]
      },
      "dependsOn": ["build-migration-datasource"]
    },
    "migrations-run": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "cwd": ".",
        "commands": [
          "typeorm -d dist/libs/database-entities-datasource/main.js migration:run"
        ]
      },
      "dependsOn": ["build-migration-datasource"]
    }
  }
}
